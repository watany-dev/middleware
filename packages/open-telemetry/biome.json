{
	"$schema": "https://biomejs.dev/schemas/1.4.1/schema.json",
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"all": true
	  }
	},
	"formatter": {
	  "enabled": true,
	  "formatWithErrors": false,
	  "ignore": [],
	  "indentStyle": "space"
	},
	"json": {
	  "parser": {
		"allowComments": true
	  },
	  "formatter": {
		"enabled": true,
		"indentStyle": "space"
	  }
	},
	"javascript": {
	  "formatter": {
		"enabled": true,
		"indentStyle": "space"
	  }
	}
  }